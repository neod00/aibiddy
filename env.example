# 나라장터 API 설정
REACT_APP_NARA_API_KEY=your_nara_api_key_here
REACT_APP_NARA_API_URL=https://apis.data.go.kr/1230000/ad/BidPublicInfoService

# OpenAI API 설정
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Gmail 이메일 설정
REACT_APP_GMAIL_USER=your-gmail@gmail.com
REACT_APP_GMAIL_APP_PASSWORD=your-16-digit-app-password

# 이메일 발송 API 설정 (SendGrid - 대안)
REACT_APP_EMAIL_API_KEY=your_email_api_key_here
REACT_APP_EMAIL_SERVICE=gmail
REACT_APP_FROM_EMAIL=your-gmail@gmail.com

# Google Sheets API 설정
REACT_APP_GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key_here
REACT_APP_GOOGLE_SHEETS_ID=your_google_sheets_id_here

# JWT 설정
REACT_APP_JWT_SECRET=your_jwt_secret_here
REACT_APP_JWT_EXPIRES_IN=7d

# 앱 설정
REACT_APP_APP_NAME=AI낙찰이
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Netlify Functions 설정
REACT_APP_NETLIFY_FUNCTIONS_URL=/.netlify/functions
